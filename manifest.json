{
    "name": "Ingredient Inspector",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popups/defaultPopup.html"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": ["contentScript.js"]
    }]
}