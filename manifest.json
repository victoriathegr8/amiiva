{
    "name": "Ingredient Inspector",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "scripts/background.js"
    },
    "action": {
        "default_popup": "popups/defaultPopup.html"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": ["scripts/contentScript.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["img/mainCheckmarkIcon.svg", "img/mainFailedIcon.svg", "img/miniCheckmarkIcon.svg", "img/miniFailedIcon.svg"],
        "matches": ["https://*/*"]
    }]
}